
<%= form_for :blog, url: blogs_path do |b| %>
  <p>
    <%= b.label :title %><br>
    <%= b.text_field :title %>
  </p>
 
  <p>
    <%= b.label :text %><br>
    <%= b.text_area :text %>
  </p>
 
  <p>
    <%= b.submit %>
  </p>

  <%= link_to 'New Blog', new_blog_path %>
  <%= link_to 'Back', blogs_path %>

<% end %>


