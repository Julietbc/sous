<h1>SousREVIEW</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @reviews.each do |review| %>
    <tr>
      <td><%= review.title %></td>
      <td><%= review.text %></td>
      <td><%= link_to 'Show', review_path(review) %></td>
      <td><%= link_to 'Edit', edit_review_path(review) %></td>
       <td><%= link_to 'Destroy', review_path(review),
              method: :delete,
              data: { confirm: 'Are you sure?' } %> 
            </td>
    </tr>
  <% end %>
   <%= link_to 'New Review', new_review_path %>
</table>